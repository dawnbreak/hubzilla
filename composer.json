{
	"name" : "dawnbreak/hubzilla",
	"type" : "application",
	"description" : "Hubzilla is a powerful platform for creating interconnected websites featuring a decentralized identity, communications, and permissions framework built using common webserver technology.",
	"keywords" : [
		"CMS",
		"identity",
		"decentralisation",
		"permission",
		"SSO",
		"ZOT"
	],
	"homepage" : "http://github.com/redmatrix/hubzilla",
	"license" : "MIT",
	"authors" : [{
			"name" : "Mike Macgirvin",
			"role" : "developer, founder"
		}, {
			"name" : "Klaus Weidenbach",
			"email" : "Klaus.Weidenbach@gmx.net",
			"homepage" : "http://redtest.dszdw.net/channel/klaus",
			"role" : "no idea"
		}
	],
	"support" : {
		"issues" : "https://github.com/redmatrix/hubzilla/issues",
		"source" : "https://github.com/redmatrix/hubzilla"
	},
	"require" : {
		"dawnbreak/hubzilla" : "dev-composer",
		"php" : ">=5.5",
		"ext-curl" : "*",
		"ext-gd" : "*",
		"ext-mbstring" : "*",
		"ext-xml" : "*",
		"ext-openssl" : "*",
		"sabre/dav" : "~3.2"
	},
	"require-dev" : {
		"php" : ">=5.6",
		"phpunit/phpunit" : "^5.6",
		"sebastian/phpcpd" : "@stable",
		"phpmd/phpmd" : "@stable",
		"pdepend/pdepend" : "@stable",
		"phploc/phploc" : "@stable",
		"squizlabs/php_codesniffer" : "@stable",
		"behat/behat" : "@stable",
		"behat/mink-extension" : "@stable",
		"behat/mink-goutte-driver" : "@stable"
	},
	"autoload" : {
		"psr-4" : {
			"Hubzilla\\" : "include/",
			"Zotlabs\\" : "Zotlabs/"
		}
	},
	"minimum-stability" : "stable",
	"repositories" : [{
			"type" : "vcs",
			"url" : "https://github.com/dawnbreak/hubzilla"
		}
	],
	"config" : {
		"notify-on-install" : false
	}
}